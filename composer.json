{
    "name": "ollico/laravel-audit-log-queue",
    "description": "A Laravel audit log queue.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David Bonner",
            "email": "david@ollicosoftware.com"
        },
        {
            "name": "Keiron McGivern",
            "email": "keironmcgivern@ollicosoftware.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/config": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "davidianbonner/enumerated": "^2.0|^3.0|^4.0",
        "spatie/laravel-activitylog": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ollico\\AuditLog\\AuditLogServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Ollico\\AuditLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ollico\\AuditLog\\Tests\\": "tests/"
        }
    }
}
