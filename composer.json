{
  "name": "ollico/laravel-audit-log-queue",
  "description": "A Laravel audit log queue.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "David Bonner",
      "email": "david@ollicosoftware.com"
    },
    {
      "name": "Keiron McGivern",
      "email": "keironmcgivern@ollicosoftware.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/config": "^10.0|^11.0",
    "illuminate/contracts": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0",
    "davidianbonner/enumerated": "^4.0.2",
    "spatie/laravel-activitylog": "^4.0.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ollico\\AuditLog\\AuditLogServiceProvider"
      ],
      "aliases": {}
    }
  },
  "autoload": {
    "psr-4": {
      "Ollico\\AuditLog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ollico\\AuditLog\\Tests\\": "tests/"
    }
  }
}
